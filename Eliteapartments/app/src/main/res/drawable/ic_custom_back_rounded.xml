<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/gray" />
            <corners android:radius="@dimen/_16dp" />
            <padding
                android:bottom="@dimen/_4dp"
                android:left="@dimen/_4dp"
                android:right="@dimen/_4dp"
                android:top="@dimen/_4dp" />
        </shape>
    </item>
    <item>
        <shape
            android:padding="@dimen/_10dp"
            android:shape="rectangle">
            <solid android:color="@color/gray" />
            <corners android:radius="@dimen/_50dp" />
        </shape>
    </item>
    <item
        android:width="@dimen/_23dp"
        android:height="@dimen/_23dp"
        android:drawable="@drawable/ic_custom_back"
        android:gravity="center" />
</layer-list>
